{
  "css_contract": {
    "selectors": {
      ".btn": [
        "border: 1px solid var(--border-soft); background: #fff; color: var(--text-main); padding: 10px 16px; border-radius: var(--radius-m); font-size: 13px; cursor: pointer; transition: background var(--transition-fast) var(--ease-soft), border-color var(--transition-fast) var(--ease-soft)",
        "transition: none"
      ],
      ".btn.small": [
        "padding: 6px 10px; font-size: 12px"
      ],
      ".n3-layout-col": [
        "display: flex; flex-direction: column; gap: var(--n3-layout-column-gap); flex: 1; min-width: 220px"
      ],
      ".n3-layout-main-pane": [
        "display: flex; flex-direction: column; gap: var(--n3-layout-gap); min-width: 0"
      ],
      ".n3-layout-row": [
        "display: flex; flex-direction: row; flex-wrap: wrap; gap: var(--n3-layout-row-gap); align-items: stretch"
      ],
      ".n3-layout-sidebar": [
        "border: 1px solid #eaeaea; border-radius: 10px; background: #fff; padding: 10px; display: flex; flex-direction: column; gap: 10px; max-height: calc(100vh - 280px); overflow: auto",
        "display: grid; gap: var(--n3-layout-sidebar-gap); grid-template-columns: minmax(0, var(--n3-layout-sidebar-width)) minmax(0, 1fr); align-items: start",
        "display: none"
      ],
      ".n3-layout-sidebar-pane": [
        "display: flex; flex-direction: column; gap: var(--n3-layout-gap)"
      ],
      ".n3-layout-stack": [
        "display: flex; flex-direction: column; gap: var(--n3-layout-gap)"
      ],
      ".n3-layout-sticky": [
        "position: sticky; z-index: 5; background: var(--n3-layout-sticky-bg); backdrop-filter: blur(6px); padding: 8px; border-radius: 10px; display: flex; flex-direction: column; gap: var(--n3-layout-gap)"
      ],
      ".topbar": [
        "position: sticky; top: 0; z-index: 10; display: flex; align-items: center; justify-content: space-between; padding: 12px 0; background: #f8f8f8; border-bottom: 1px solid var(--border-subtle)"
      ],
      ".ui-chat": [
        "display: flex; flex-direction: column; gap: 14px"
      ],
      ".ui-chat-composer": [
        "display: flex; flex-wrap: wrap; gap: 8px"
      ],
      ".ui-chat-message": [
        "display: flex; gap: 8px; align-items: flex-end"
      ],
      ".ui-chat-messages": [
        "display: flex; flex-direction: column; gap: 6px"
      ],
      ".ui-list": [
        "display: flex; flex-direction: column; gap: 8px"
      ],
      ".ui-list-item": [
        "display: flex; align-items: center; gap: 10px; border: 1px solid #eaeaea; border-radius: 8px; padding: 8px"
      ],
      ".ui-scope-option": [
        "border: 1px solid #ccd2dc; border-radius: 999px; background: #fff; color: #223; padding: 4px 10px; font-size: 12px; cursor: pointer"
      ],
      ".ui-scope-selector": [
        "border: 1px solid #eaeaea; border-radius: 10px; padding: 10px; display: flex; flex-direction: column; gap: 8px"
      ],
      ".ui-section": [
        "background: #fff; display: flex; flex-direction: column; gap: 10px"
      ],
      ".ui-section-title": [
        "font-size: 14px; font-weight: 600; letter-spacing: 0.2px"
      ],
      ".ui-upload": [
        "display: flex; flex-direction: column; gap: 10px"
      ],
      ".ui-upload-file-item": [
        "border: 1px solid #eaeaea; border-radius: 8px; padding: 8px; display: flex; align-items: center; justify-content: space-between; gap: 8px"
      ],
      ".ui-upload-status": [
        "font-size: 12px; color: #555"
      ]
    },
    "sticky_topbar": true
  },
