spec is "1.0"

packs:
  "builtin.http"

tool "get json from web":
  implemented using python

  input:
    url is text

  output:
    status is number
    headers is json
    data is json

flow "demo":
  try:
    let response is get json from web:
      url is "https://example.com/data"
    return "unexpected"
  with catch err:
    return "blocked"
